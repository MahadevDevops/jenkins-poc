pipeline {
    agent any
    
    environment {
        TF_VERSION = "0.15.5" // Specify the Terraform version you want to use
       
    }
    
    stages {
        stage('Connect-ec2') {
            steps {
                echo "Get the public ip of ec2"
                sh "terraform init"
                sh "terraform output ec2_public_IP"
               
            }
        }

    }
    

}